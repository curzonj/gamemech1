{
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [".js"]
        }
      ]
    }
}
