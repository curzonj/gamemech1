{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-param-reassign": 0,
    "no-console": 0,
    "react/destructuring-assignment": 0,
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js"]
      }
    ]
  }
}
